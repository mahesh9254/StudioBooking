    <!-- Begin clients -->
    <section class="client-section shuffle-client dark-section v-separator pt-xs-50 pt-md-50 pt-lg-120 pb-xs-50 pb-md-50 pb-lg-120">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="section-header white">
                        <h2>We Worked With</h2>
                    </div>
                    @*<div class="client-filter">
                    <ul class="client-filter-list white" data-for-filter="#js-clientFilter">
                    <li class="client-filter-list__item"><a href="#" class="active" data-filter="*">All Clients</a></li>
                    <li class="client-filter-list__item"><a href="#" data-filter="music">Music</a></li>
                    <li class="client-filter-list__item"><a href="#" data-filter="post-roduction">Post Production</a></li>
                    <li class="client-filter-list__item"><a href="#" data-filter="feature-films">Feature Films</a></li>
                    </ul>
                    </div>*@

                    @{
                        var clientsInChunk = Model.Clients.Select(c => c.Name).Take(28).GetChunks(7);
                    }
                    <div class="row" id="js-clientFilter">
                        @foreach (var chunk in clientsInChunk)
                        {
                            <div class="col-6 col-md-4 col-lg-3 @(count == 1 ? "" : count == clientsInChunk.Count ? "collapse-clients collapsed-sm" : "collapse-clients collapsed-xs")">
                                <ul class="clients-list">
                                    @foreach (var name in chunk)
                                    {
                                        <li>@name</li>
                                    }
                                </ul>
                            </div>
                            count = count + 1;
                        }

                        @*<ul class="tt-pagination pagination_white">
                        <li><a href="#" class="active">1</a></li>
                        <li><a href="#">2</a></li>
                        <li><a href="#">3</a></li>
                        <li><p>...</p></li>
                        <li><a href="#">6</a></li>
                        </ul>*@
                    </div>
                </div>
            </div>
        </div>
        <div class="section-bg op-1 lazy" data-src="/lib/voicer/img/clients-bg.jpg"></div>
    </section>
    <!-- End clients -->
    <!-- Begin review -->
    <section class="white-section parralax-section v-separator pt-xs-25 pt-md-25 pt-lg-60 pb-xs-25 pb-md-25 pb-lg-60">
