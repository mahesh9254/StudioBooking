    <!-- Begin testimonials -->
    <section class="dark-section testimonial-section v-separator pt-xs-40 pt-md-50 pt-lg-100 pb-xs-40 pb-md-50 pb-lg-100">
        <img src="~/lib/voicer/img/quote.png" class="quote-img" alt="img">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-12 col-sm-6">
                    <div class="testimonials-slider white-arrow"
                         data-slick='{
                                     "slidesToShow": 1,
                                     "swipeToSlide": true,
                                     "adaptiveHeight": true,
                                     "responsive": [
                                         {
                                             "breakpoint": 992,
                                             "settings": {
                                                 "slidesToShow": 1,
                                                 "arrows": false,
                                                 "adaptiveHeight": true,
                                                 "dots": true,
                                                "autoplay": true,
                                                 "autoplaySpeed": 3500
