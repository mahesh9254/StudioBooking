    <section class="white-section parralax-section v-separator pb-xs-25 pb-md-25 pb-lg-60">
        <div class="text-section ovh pt-xs-25 pt-md-25 pt-lg-60 pb-xs-25 pb-md-25 pb-lg-60">
            <div class="container">
                <div class="row flex-row-reverse align-items-center">
                    <div class="col-md-6">
                        <div class="text-section_video">
                            <h2>
                                Welcome to  R & B Studios
                            </h2>
                            <a data-fancybox href="/" class="img-wrap js-tilt">
                                <img src="data:image/gif;base64,R0lGODlhAQABAAAAACwAAAAAAQABAAA=" class="text-img lazy" data-src="./lib/voicer/img/text-block-video-1.png" alt="bg">
                                @*<span class="start-video-wrap">
                                <span class="start-video">
                                <svg width="17" height="21" viewBox="0 0 17 21" xmlns="http://www.w3.org/2000/svg">
