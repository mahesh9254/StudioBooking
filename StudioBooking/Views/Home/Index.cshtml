                                <div class="tt-slide">
                                    <div class="text-block white">
                                        <h2>What our Customers Say</h2>
                                        <p>
                                            @Html.Raw(testimonial.Review)
                                        </p>
                                        <h5 class="theme-color mb-0">– @testimonial.Name</h5>
                                    </div>
                                </div>
                            }
                        }
