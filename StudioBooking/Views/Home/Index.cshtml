    </section>
    <!-- End record studio -->
    <!-- Begin service-section -->
    <section class="block-bg-projects parralax-section v-separator pt-xs-25 pt-md-25 pt-lg-60 pb-xs-25 pb-md-25 pb-lg-60">
        <div class="ovh">
            <div class="container pt-xs-25 pt-md-25 pt-lg-60 pb-xs-25 pb-md-25 pb-lg-60">
                <div class="row">
                    <div class="col-12">
                        <div class="section-header">
                            <h2>Our Services</h2>
                            <p>Our services are tailored to meet the needs of musicians, artists, content creators and businesses seeking to produce top notch audio content.</p>
                        </div>
                        <div class="project-slider arrow-outside"
                             data-slick='{
                                         "slidesToShow": 3,
                                         "swipeToSlide": true,
                                         "responsive": [
                                             {
                                                 "breakpoint": 1200,
                                                 "settings": {
                                                     "slidesToShow": 3,
                                                     "arrows": false,
                                                     "dots": true
                                                 }
                                             },
                                             {
                                                 "breakpoint": 768,
                                                 "settings": {
                                                     "slidesToShow": 2,
                                                     "arrows": false,
                                                     "dots": true,
                                                    "autoplay": true,
                                   "autoplaySpeed": 3500
                                                 }
                                             },
                                             {
                                                 "breakpoint": 481,
                                                 "settings": {
                                                     "slidesToShow": 1,
                                                     "arrows": false,
                                                     "dots": true,
                                                    "autoplay": true,
                                   "autoplaySpeed": 3500
                                                 }
                                             }
                                         ]
                                     }'>
                            @foreach (var service in Model.Services.Where(c => c.ServiceType == Enums.ServiceType.Calculated))
                            {
                                <div class="tt-slide">
                                    <div class="project-card">
                                        <a href="/Service/Detail/@service.Id" class="project-cart_logo">
                                            <picture>
                                                @if (service.Name == "Rehearsal")
                                                {
                                                    <source type="image/jpeg" srcset="~/lib/studiobooking/img/service/bg_studio_1-384x216xc.jpg">
                                                    <img src="~/lib/studiobooking/img/service/bg_studio_1-384x216xc.jpg" alt="Product Images">
                                                }
                                                @if (service.Name == "DIY Audio/Video Shoot")
                                                {
                                                    <source type="image/jpeg" srcset="~/lib/studiobooking/img/service/bg_studio_4-384x216xc.jpg">
                                                    <img src="~/lib/studiobooking/img/service/bg_studio_4-384x216xc.jpg" alt="Product Images">
                                                }
                                                @if (service.Name == "Audio Recording")
                                                {
                                                    <source type="image/jpeg" srcset="~/lib/studiobooking/img/service/bg_studio_2-384x216xc.jpg">
                                                    <img src="~/lib/studiobooking/img/service/bg_studio_2-384x216xc.jpg" alt="Product Images">
                                                }
                                                @if (service.Name == "Live Streaming")
                                                {
                                                    <source type="image/jpeg" srcset="~/lib/studiobooking/img/service/bg_studio_3-384x216xc.jpg">
                                                    <img src="~/lib/studiobooking/img/service/bg_studio_3-384x216xc.jpg" alt="Product Images">
                                                }
                                                @if (service.Name.Contains("Bundles"))
                                                {
                                                    <source type="image/jpeg" srcset="~/lib/studiobooking/img/service/bg_event_space-384x216xc.jpg">
                                                    <img src="~/lib/studiobooking/img/service/bg_event_space-384x216xc.jpg" alt="Product Images">
                                                }
                                            </picture>
                                        </a>
                                        <a href="/Service/Detail/@service.Id" class="project-cart_name">@service.Name</a>
                                        @*<p class="theme-color">@service.</p>*@
                                    </div>
                                </div>
                            }
