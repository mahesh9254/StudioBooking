@using StudioBooking.Infrastructure
@using StudioBooking.ViewModels;
@model StudioBooking.ViewModels.StudioViewModel
@{
    ViewData["Title"] = "Home Page";
    var layoutViewModel = (LayoutViewModel)(ViewData["LayoutViewModel"] ?? new LayoutViewModel());
    var count = 1;
}
@section Style{
    <style type="text/css">
        .text-section .text-section_video h2::after {
            content: none !important;
        }
    </style>
}
<!-- Begin main-slider -->
<div class="main-slider-wrap">
    <div class="main-slider"
         data-slick='{
                     "arrows": false,
                     "fade": true,
                     "speed": 200
                 }'>
        <div class="tt-slide">
            <div class="slide-bg" data-src="/lib/voicer/img/main-slider-1.jpg">
                <img src="/lib/voicer/img/main-slider-1.jpg" alt="bg">
            </div>
            <div class="container">
                <div class="row">
                    <div class="col-12 text-center">
                        <p class="tt-slide_description" style="font-size:larger!important">· Rehearsal · Recording · Mixing </p>
                        @{
                            var taglineText1 = layoutViewModel.WebsiteSetting.TagLine.Replace(layoutViewModel.WebsiteSetting.TagLine.Split(" ").LastOrDefault(), "");
                        }
                        <p class="tt-slide_header"><span class="font-gradient">@taglineText1</span><br>@layoutViewModel.WebsiteSetting.TagLine.Split(" ").LastOrDefault()</p>
                        <a href="javascript:;" class="button">Book Now</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End main-slider -->
