    </section>
    <!-- End text-section -->
    <!-- Begin record studio -->
    <section class="dark-section v-separator pt-xs-50 pt-md-50 pt-lg-120 pb-xs-50 pb-md-50 pb-lg-120">
        <div class="container">
            <div class="section-header white">
                <h2>Our Studios</h2>
                <p> R & B Studios is a 1500 square feet facility with 3 live rooms and 2 control rooms.</p>
            </div>
        </div>
        <div class="studio-slider white-arrow"
             data-slick='{
                         "centerMode": true,
                         "centerPadding": "29.5%",
                         "responsive": [
                             {
                                 "breakpoint": 1400,
                                 "settings": {
                                   "centerPadding": "22%"
                                 }
                             },
                             {
                                 "breakpoint": 768,
                                 "settings": {
                                   "centerPadding": "0%",
                                   "arrows": false,
                                   "dots": true,
                                   "autoplay": true,
                                   "autoplaySpeed": 3500
                                 }
                             }
                         ]
                     }'>
            @foreach (var category in Model.Categories)
            {
               
                <div class="tt-slide">
                    <a href="/Home/OurStudios">
                    <div class="studio-info">
                        <div class="studio-info_text">
                            <h4><span class="color-main"></span></h4>
                            <div class="studio-info_footer">
                                <p>@category.Name</p>
                                <p>@category.Title</p>
                                <p>@category.Description</p>
                            </div>
                        </div>
                        <picture>
                            <source type="image/jpeg" srcset="@(AppConfig.CategoryImageUrl + category.Image)">
                            <img src="@(AppConfig.CategoryImageUrl + category.Image)" class="studio-photo" alt="img">
                           </picture>
                    </div>
                    </a>
                </div>
                
            }
        </div>
